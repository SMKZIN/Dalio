generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Hero {
  id Int  @default(autoincrement()) @id
  name String
  power Float
  country String
  ability String
}

model Users {
  id       String @id @default(uuid())
  age      Int 
  name     String
  email    String @unique
  password String

  @@map("users") 
}
